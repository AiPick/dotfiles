nnoremap H :tabprevious<CR>
nnoremap L :tabnext<CR>
nnoremap D :tabclose!<CR>
set hintchars=abcdefghijklmnopqrstuvwxyz
set focuscontent
colorscheme indigo

source /home/farseer/.vimperator/vimperator-plugins/_libly.js
source /home/farseer/.vimperator/vimperator-plugins/feedSomeKeys_3.js

noremap <silent> f :!fcitx-remote -c<cr>qf
noremap <silent> F :!fcitx-remote -c<cr>qF

fmaps -urls='feedly.com' j k <s-j> <s-k> n p o v r <s-m> gg ga
" vim:ts=4:sw=4:tw=0:ft=vim
