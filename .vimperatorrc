source /home/farseer/.vimperator/vimperator-plugins/feedSomeKeys_3.js


fmaps -urls='feedly.com' j k <s-j> <s-k> n p o v r <s-m> gg ga
" vim:ts=4:sw=4:tw=0:ft=vim
